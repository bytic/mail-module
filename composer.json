{
  "name": "bytic/mail-module",
  "type": "library",
  "description": "Mail Module for bytic framework",
  "homepage": "https://github.com/bytic/mail-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\MailModule\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\MailModule\\Tests\\": "tests/src",
      "Nip\\MailModule\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "bytic/database": "^1.0",
    "bytic/orm": "^1.0",
    "bytic/mail": "^1.0",
    "bytic/medialibrary": "^0.9",
    "bytic/filesystem": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "mockery/mockery": "^1.2"
  }
}
