{
  "name": "bytic/mail-module",
  "description": "Mail Module for bytic framework",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "homepage": "https://github.com/bytic/mail-module",
  "require": {
    "ext-json": "*",
    "bytic/mail": "^0.9 || ^1.0",
    "bytic/actions": "dev-main",
    "bytic/medialibrary": "^0.8 || ^0.9",
    "bytic/orm": "^0.9 || ^1.0",
    "bytic/orm-behaviors": "^0.9 || ^1.0",
    "bytic/orm-filters": "^0.9 || ^1.0",
    "bytic/package-base": "^1.0"
  },
  "require-dev": {
    "bytic/cache": "^0.9 || ^1.0",
    "bytic/event-dispatcher": "^0.9 || ^1.0",
    "bytic/translation": "^0.9 || ^1.0",
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Nip\\MailModule\\": "src"
    },
    "classmap": [
      "legacy/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\MailModule\\Tests\\": "tests/src",
      "Nip\\MailModule\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  },
  "scripts": {}
}
